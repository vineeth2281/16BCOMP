/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a = a, b = b, out = nab);
    Nand(a = a, b = nab, out = na);
    Nand(a = nab, b = b, out = nb);
    Nand(a = na, b = nb, out = out);
}
