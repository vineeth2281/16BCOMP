/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = sel, out = SEL);
    And(a = a, b = SEL, out = aSEL);
    And(a = b, b = sel, out = bsel);
    Or(a = bsel, b = aSEL, out = out);
}
